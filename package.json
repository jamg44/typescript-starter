{
	"name": "typescript-starter",
	"version": "1.0.0",
	"description": "A TypeScript starter project",
	"author": "jamg44",
	"repository": "jamg44/typescript-starter",
	"license": "MIT",
	"main": "index.ts",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "rimraf dist && tsc && tsc-alias",
		"dev": "node --import=tsx --watch src/index.ts",
		"start:prod": "node dist/index.js",
		"check": "biome check --write",
		"test": "vitest run",
		"test:watch": "vitest watch",
		"test:cov": "vitest run --coverage"
	},
	"dependencies": {
		"dotenv": "17.2.3",
		"pino": "10.1.0",
		"tsc-alias": "^1.8.16",
		"zod": "4.1.12"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.2",
		"@types/node": "^24.9.2",
		"@vitest/coverage-v8": "4.0.6",
		"pino-pretty": "13.1.2",
		"rimraf": "^6.1.0",
		"tsx": "4.20.6",
		"typescript": "5.9.3",
		"vite-tsconfig-paths": "5.1.4",
		"vitest": "4.0.6"
	},
	"packageManager": "pnpm@10.14.0"
}
